{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
        "no-trailing-whitespace": false,
        "no-input-rename": false,
        "no-inferrable-types": false
    }
}

